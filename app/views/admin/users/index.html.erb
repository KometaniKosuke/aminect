<% if flash.notice %>
  <p class="notice"><%= flash.notice %></p>
<% end %>

<table border=5 width="500px">
  <tr align="center">
    <th>ID</th>
    <th>名前</th>
  </tr>
  <% @users.each do |user| %>
      <tr align="center">
        <td><%= user.id %></td>
        <td>
          <%= link_to user.name, [:admin, user] %>
        </td>
      </tr>
  <% end %>
</table>
<br>
<%= form_tag :term_change_admin_users, method: :patch do %>
  <%= submit_tag "利用規約更新" %>
<% end %>
<br><br>
